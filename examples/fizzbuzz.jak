func fizzBuzz(number) {
    var fizz = "fizz";
    var buzz = "buzz";

    if (number % 3 == 0 && number % 5 == 0) {
        println(fizz, buzz);
    } elif (number % 3 == 0) {
        println(fizz);
    } elif (number % 5 == 0) {
        println(buzz);
    } else {
        println(number);
    }
}